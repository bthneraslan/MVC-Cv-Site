@using CvSitem.Models.Siniflar
@model List<İletisim>
@{
    ViewBag.Title = "Hobiler";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Mesajlar</h2>
<br />


<table class="table table-hover">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Sil</th>
    </tr>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <th>@i.ID</th>
                <td>@i.AdSoyad</td>
                <td>@i.Konu</td>
                <td>@i.Mesaj</td>
                <td><a href="/Admin/MesajSil/@i.ID" class="btn btn-danger">Sil</a></td>
            </tr>
        }
    </tbody>
</table>